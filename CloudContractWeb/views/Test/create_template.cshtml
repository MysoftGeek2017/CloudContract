@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <title></title>
    <script src="~/static/3rd/jquery-1.9.1.js" type="text/javascript"></script>

    <link href="~/static/3rd/Office.css" rel="stylesheet" type="text/css" />
    <script src="https://appsforoffice.microsoft.com/lib/beta/hosted/office.js" type="text/javascript"></script>

    <!-- 要允许使用 Office.js 的本地引用进行脱机调试，请使用:                        -->
    <!-- <script src="../../Scripts/Office/MicrosoftAjax.js" type="text/javascript"></script>  -->
    <!-- <script src="../../Scripts/Office/1/office.js" type="text/javascript"></script>  -->

    <link href="~/static/App.css" rel="stylesheet" type="text/css" />
    <script src="~/static/App.js" type="text/javascript"></script>

    <link href="~/static/Test/create-template.css" rel="stylesheet" type="text/css" />
    <script src="~/static/Test/create-template.js" type="text/javascript"></script>
</head>
<body>
    <div id="content-header">
        <div class="padding">
            <h1>创建模板</h1>
            <div id="toolbar">
                <a class="glyphicon glyphicon-file" id="btn-save" href="javascript:void 0">保存模板</a>
            </div>

        </div>
    </div>
    <div id="content-main">
        <div class="padding">
            <h2>普通字段</h2>
            <ul>
                <li><a class="field-insert" href="javascript:void 0">合同编号</a></li>
                <li><a class="field-insert" href="javascript:void 0">合同名称</a></li>
                <li><a class="field-insert" href="javascript:void 0">甲方单位</a></li>
                <li><a class="field-insert" href="javascript:void 0">乙方单位</a></li>
                <li><a class="field-insert" href="javascript:void 0">丙方单位</a></li>
                <li><a class="field-insert" href="javascript:void 0">合同日期</a></li>
            </ul>
        </div>
        <div class="padding">
            <h2>货物列表</h2>
            <ul>
                <li><a class="field-insert" href="javascript:void 0">货物名称</a></li>
                <li><a class="field-insert" href="javascript:void 0">合同数量</a></li>
                <li><a class="field-insert" href="javascript:void 0">单价</a></li>
                <li><a class="field-insert" href="javascript:void 0">金额</a></li>
            </ul>
        </div>

        <div class="padding">
            <h2>付款方式列表</h2>
            <ul>
                <li><a href="javascript:void 0">付款比例</a></li>
                <li><a href="javascript:void 0">付款日期</a></li>
                <li><a href="javascript:void 0">金额</a></li>
            </ul>
        </div>
    </div>
</body>
</html>
