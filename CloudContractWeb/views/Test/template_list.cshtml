@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <title></title>
    <script src="~/static/3rd/jquery-1.9.1.js" type="text/javascript"></script>

    <link href="~/static/3rd/Office.css" rel="stylesheet" type="text/css" />
    <script src="https://appsforoffice.microsoft.com/lib/beta/hosted/office.js" type="text/javascript"></script>

    <!-- 要允许使用 Office.js 的本地引用进行脱机调试，请使用:                        -->
    <!-- <script src="../../Scripts/Office/MicrosoftAjax.js" type="text/javascript"></script>  -->
    <!-- <script src="../../Scripts/Office/1/office.js" type="text/javascript"></script>  -->

    <link href="~/static/App.css" rel="stylesheet" type="text/css" />
    <script src="~/static/App.js" type="text/javascript"></script>

    <link href="~/static/Test/template_list.css" rel="stylesheet" type="text/css" />
    <script src="~/static/Test/template_list.js" type="text/javascript"></script>
</head>
<body>
    <div id="content-header">
        <div class="padding">
            <h1>选择模板</h1>
            <div id="toolbar">
                <a class="glyphicon glyphicon-file" id="btn-return" href="javascript:void 0">新建模板</a>
                <a class="glyphicon glyphicon-file" id="btn-return" href="javascript:void 0">返回</a>
            </div>

        </div>
    </div>
    <div id="content-main">
        <div class="padding">
            <h2>模板列表</h2>
            <ul>
                @foreach( var name in Model ) {
                    <li><a class="template-item" href="javascript:void 0">@name</a></li>
                }
            </ul>
        </div>
    </div>
</body>
</html>
